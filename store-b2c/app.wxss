/*************************************
* name: dAlert
* tips: 自定义弹窗
**************************************/
.dAlert{position:fixed;left:0;top:0;z-index:9990;width:100%;height:100%;overflow:hidden;visibility:hidden;opacity:0;transition:all 0.3s;}
.dAlert .alertMask{position:absolute;z-index:9998;background:rgba(0,0,0,0.5);width:100%;height:100%;}
.dAlert .alertContainer{position:absolute;z-index:9999;left:50rpx;right:50rpx;transition:all 0.3s;transform:scale3d(0,0,0);border-radius:5rpx;}
.dAlert .alertContainer .alert-head{padding:15rpx 25rpx;height:60rpx;line-height:60rpx;position:relative;z-index:0;}
.dAlert .alertContainer .alert-head .fi-close{position:absolute;right:20rpx;top:50%;font-size:40rpx;margin-top:-32rpx;color:#595450;cursor:pointer;transition:all 0.3s;}
.dAlert .alertContainer .alert-head .fi-close:hover{transform:rotate(180deg);-ms-transform:rotate(180deg);-moz-transform:rotate(180deg);-webkit-transform:rotate(180deg);-o-transform:rotate(180deg);}
.dAlert .alertContainer .alert-head .title{font-size:32rpx;color:#4c4743;}
.dAlert .alertContainer .alert-main{overflow-x:hidden;overflow-y:auto;}
.dAlert .alertContainer .alert-main .alertMainer{padding:20rpx;}
.dAlert .alertContainer .alert-foot{padding:15rpx 15rpx;}
.dAlert.on{visibility:visible;opacity:1;}
.dAlert.on .alertContainer{transform:scale3d(1,1,1);}
/*************************************
* name: button
* tips: 默认按钮
**************************************/
button[disabled]::after{border-color:#ccc;color: #ccc;}

/***************************************************
* name: starGroup
* tips: 等级评分
****************************************************/
.starGroup{width:200rpx;height:44rpx;overflow:hidden;position:relative;z-index:0;}
.starGroup .starContainer{position:absolute;left:0;top:0;z-index:0;overflow:hidden;}
.starGroup .starContainer-on{z-index:1;}
.starGroup .starContainer-on .fi-star{color:#feb61a;}
.starGroup .starList{width:200rpx;}
.starGroup .fi-star{font-size:40rpx;color:#eee;}
/***************************************
* name: lineAngleUpGroup
* tips: 带线向上箭头组件
****************************************/
.lineAngleUpGroup {height: 20rpx;border-bottom: 2rpx solid #eee;position: relative;z-index: 0;}
.lineAngleUpGroup .fi-angle-up {position: absolute;left: 20rpx;bottom: -15rpx;color: #eee;font-size: 40rpx;width: 24rpx;background: #fff;}
.lineAngleUpGroup:before {content: '';display: block;position: absolute;width: 20rpx;height: 1rpx; /*no*/background: #fff;left: 29rpx;top: 20rpx;}
/*************************************
* name: showScreenView
* tips: 自定义筛选弹窗
**************************************/
.showScreenView {position: fixed;left: 0;top: 0;z-index: 9990;width: 100%;height: 100%;overflow:hidden;transition:all .3s;}
.showScreenView .screenMask {position:absolute;z-index:9998;background:rgba(0,0,0,0.5);width:100%;height:100%;}
.showScreenView .screenContainer {position: absolute;z-index:9999;left: 150rpx;right:0;top: 0;bottom: 0;animation: screenMove .5s;background: #f8f8f8;}
.showScreenView .screenContainer .btnGroup {position: absolute;bottom: 0;left: 0;right: 0; border-top: 1rpx solid #e6e6e6;box-shadow: 0 -1rpx 15rpx rgba(0, 0, 0, .2)}
.showScreenView .screenContainer .btnGroup button {border: none !important;border-radius: 0 !important;height: 90rpx; font-size: 36rpx;}
.showScreenView .screenContainer .btnGroup button.reset {background: #fff}
.showScreenView .screenContainer .btnGroup button.submit {background: #ff3306;color: #fff;}

.showScreenView .screenContainer .classfiyContainer {position: absolute;top: 0;left: 0;right: 0;bottom: 100rpx;}
.showScreenView .screenContainer .classfiyContainer .classfiy-item {background: #fff;}
.showScreenView .screenContainer .classfiyContainer .classfiy-item .classfiy-header {font-size: 30rpx;padding: 24rpx;}
.showScreenView .screenContainer .classfiyContainer .classfiy-item .classfiy-header .fi {font-size: 40rpx;color: #cccccc;transition: all .3s;}
.showScreenView .screenContainer .classfiyContainer .classfiy-item .classfiy-header .fi.show {transition: all .3s; transform: rotate(90deg);}
.showScreenView .screenContainer .classfiyContainer .classfiy-item .classfiy-tag {padding: 0 24rpx 24rpx 24rpx;}
.showScreenView .screenContainer .classfiyContainer .classfiy-item .classfiy-tag.show {display: block;}
.showScreenView .screenContainer .classfiyContainer .classfiy-item .classfiy-tag .tagGroup {margin-left: -24rpx;
  margin-top: -24rpx;flex-wrap: wrap;}
.showScreenView .screenContainer .classfiyContainer .classfiy-item .classfiy-tag .tagGroup .tag {display: inline-block;width: 168rpx; margin-left: 24rpx;margin-top: 24rpx; background: #f7f7f7;height: 60rpx;line-height: 60rpx;text-align: center;border: 1rpx solid transparent;overflow: hidden;font-size: 26rpx;}
.showScreenView .screenContainer .classfiyContainer .classfiy-item .classfiy-tag .tagGroup .tag.active {color: #eb0c36;border-color: #eb0c36;}
.showScreenView .screenContainer .classfiyContainer .classfiy-item .classfiy-tag .input {background: #f7f7f7;font-size: 26rpx;text-align: center; height: 60rpx;line-height: 60rpx;width: 250rpx;}
.showScreenView .screenContainer .classfiyContainer .classfiy-list-li {padding: 0 24rpx;height: 80rpx;font-size: 30rpx;position: relative;border-bottom: 1rpx solid #e6e6e6;}
.showScreenView .screenContainer .classfiyContainer .classfiy-list-li .icon image{width: 30rpx;height: 20rpx;}
.showScreenView .screenContainer .classfiyContainer .classfiy-list-li .nav-back {position: absolute;}
.showScreenView .screenContainer .classfiyContainer .classfiy-list-li .fi {font-size: 36rpx;font-weight: bold;}

@keyframes screenMove {0%{transform: translateX(150%);}}