/**app.wxss**/
@import "commons/extension/extension.wxss";
@import "wxSortPickerView/wxSortPickerView.wxss";
@import "area_picker/area_picker.wxss";

page {
  height: 100%;
  font-size: 30rpx;
  color: #333;
  background: #f0f0f0;
  overflow-x: hidden;
}

page, block, view, scroll-view, swiper, movable-view, icon, text, progress,
button, checkbox, form, input, label, picker, picker-view, radio, slider, switch,
textarea, navigator, audio, image, video, map, canvas, contact-button {
  box-sizing: border-box;
}

button {
  font-size: 11pt;
  font-family: inherit;
}


.hide {display: none;visibility: hidden;}

.no-scroll {
    height: 100%;
    overflow-y: hidden;
}

.flex {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}

.flex-row {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  flex-direction: row;
}

.flex-col {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  flex-direction: column;
}

.flex-grow-0 {
  min-width: 0;
  -webkit-box-flex: 0;
  -ms-flex-positive: 0;
  flex-grow: 0;
  -ms-flex-negative: 0;
  flex-shrink: 0;
}

.flex-grow-1 {
  min-width: 0;
  -webkit-box-flex: 1;
  -ms-flex-positive: 1;
  flex-grow: 1;
  -ms-flex-negative: 1;
  flex-shrink: 1;
}

.flex-x-center {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -ms-flex-pack: center;
  justify-content: center;
}

.flex-x-between {
  justify-content: space-between;
}

.flex-y-center {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -ms-flex-align: center;
  -ms-grid-row-align: center;
  align-items: center;
}

.flex-y-bottom {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: end;
  -ms-flex-align: end;
  -ms-grid-row-align: flex-end;
  align-items: flex-end;
}

.bg-white {
  background: #fff;
} 

.mb-20 {
  margin-bottom: 20rpx;
}

.mb-10 {
  margin-bottom: 10rpx;
}

/* 顶部搜索 */

.header {
  width: 100%;
  height: 100rpx;
  color: white;
  position: fixed;
  z-index: 2000;
  left: 0;
  top: 0;
}

.header .search-block {
  padding: 0 20rpx;
}

.header .search-block navigator {
  position: relative;
  background: rgba(255, 255, 255, 0.4);
  height: 64rpx;
  font-size: 30rpx;
  border-radius: 64rpx;
}

.header .search-block navigator .fi {
  margin: 0 15rpx;
  font-size: 50rpx;
}

.header .message-block {
  margin-right: 20rpx;
}

.header .message-block .fi {
  font-size: 64rpx;
}

.pd-top-100 {
  padding-top: 100rpx;
}

/* 自定义导航 */

.navbar {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  background: #fff;
  color: #555;
  z-index: 2000;
  border-top: 1rpx solid rgba(0, 0, 0, 0.1);
  box-sizing: border-box;
}

.navbar navigator {
  height: 115rpx;
  width: 1%;
}

.navbar navigator>view {
  width: 100%;
  padding: 8rpx 0;
}

.navbar .navbar-icon {
  width: 48rpx;
  height: 48rpx;
  display: block;
  margin: 0 auto;
}

.navbar .navbar-text {
  font-size: 8pt;
  text-align: center;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  margin-top: 8rpx;
}

.hidden {
  display: none;
}

.navbar button {
  display: block;
  padding: 0;
  border: 0;
  background: none;
  margin: 0;
  width: 100%;
  line-height: 1.25;
}

.navbar button::after {
  display: none;
}

.navbar.device_iphone_x {
  padding-bottom: 65rpx;
}

.navbar+.after-navber {
  padding-bottom: 115rpx;
}

.page.show_navbar>.body {
  padding-bottom: 115rpx;
}

.page.show_navbar.device_iphone_x>.header .navbar {
  padding-bottom: 65rpx;
}

.page.show_navbar.device_iphone_x>.body {
  padding-bottom: 180rpx;
}

.text-more-2 {
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  word-break: break-all;
}

/* 数据加载 */

.loading {
  width: 100%;
  color: #999;
  font-size: 28rpx;
  margin: 20rpx 0;
}

.loading image {
  width: 36rpx;
  height: 36rpx;
  margin-right: 20rpx;
}

