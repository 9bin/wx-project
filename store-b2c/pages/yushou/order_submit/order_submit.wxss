/* pages/yushou/order_submit/order_submit.wxss */
view {
    overflow: visible;
}

.bottom-bar {
    position: fixed;
    bottom: 0;
    left: 0;
    height: 100rpx;
    background: #fff;
    border-top: 1rpx solid #e3e3e3;
    width: 100%;
    z-index: 1000;
}

.is_area {
    background: #FEBD14;
    color: #ffffff;
    display: flex;
    align-items: center;
    justify-content: center;
    width: 100%;
}

.bottom-bar .submit-btn {
    background-color: #eb0c36;
    color: #fff;
    width: 250rpx;
    text-align: center;
    margin: 0;
    padding: 0;
    border-radius: 0;
}

.address-picker {
    background: #fff;
    padding: 32rpx 24rpx;
    margin-bottom: 20rpx;
    background-image: url("data:image/png;base64,");
    background-position:left bottom;
    background-repeat:repeat-x;
    background-size:35rpx 5rpx;
}

.cart-item {
    background: #fff;
    padding: 32rpx 24rpx 10rpx 24rpx;
}

.cart-item .goods-pic {
    width: 156rpx;
    height: 156rpx;
    margin-right: 20rpx;
    border: 1rpx solid #eee
}

.cart-item .goods-name {
    margin-bottom: 10rpx;
    -webkit-line-clamp: 2;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    overflow: hidden;
}

.cart-item .attr-list,.cart-item .num {
    font-size: 9pt;
    color: #999;
}

.cart-item .attr-list .attr-item {
    display: inline-block;
    margin-right: 25rpx;
}

.cart-item .attr-list .attr-item:last-child {
    margin-right: 0;
}

.cart-item .orgin_price {
    color: #666;
    font-size: 24rpx;
}

.goods-intrgral {
    height: 60rpx;
    background-color: #f8f8f8;
    width: 100%;
    border-bottom: 1rpx solid #e3e3e3;
    color: #919191;
    font-size: 9pt;
    padding: 0 24rpx;
}

.goods-gifts {
    height: 60rpx;
    background-color: #f8f8f8;
    width: 100%;
    border-bottom: 1rpx solid #e3e3e3;
    color: #919191;
    font-size: 9pt;
    padding: 0 24rpx;
}

.lable {
  width: 100%;
  padding: 24rpx;
  background: #fff;
  font-size: 28rpx;
}