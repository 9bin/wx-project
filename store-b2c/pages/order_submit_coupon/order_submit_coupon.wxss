/* pages/order_submit_coupon/order_submit_coupon.wxss */
.list {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 50rpx;
}
.radio {
  width: 94%;
  background: #fff;
  margin: 20rpx auto 0;
  height: 200rpx;
}
.radio-left {
  position: relative;
  width: 240rpx;
  background: -webkit-linear-gradient(top,#f519a5,#f21b43) !important;
  color: white;
  height: 200rpx;
}
.radio-left::before {
  display: block;
  content: '';
  position: absolute;
  top: 0;
  right: -5rpx;
  width: 10rpx;
  height: 100%;
  background: url("data:image/png;base64,") center center repeat-y;
  background-size: 10rpx auto;
}

.radio-middle {
  margin: 0 24rpx;
}

.radio-middle .name {
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  display: -webkit-box;
  text-overflow: ellipsis;
  overflow: hidden;
  font-size: 26rpx;
  color: #666;
  margin-bottom: 20rpx;
  line-height: 1.1rem;
}

.radio-middle .name .icon {
  background: #eb0c36;
  border-radius: 5rpx;
  color: #fff;
  font-size: 22rpx;
  padding: 0 8rpx;
  margin-right: 15rpx;
}