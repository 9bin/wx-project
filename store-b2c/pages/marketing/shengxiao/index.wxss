.lotto-center .lotto-main .item .side-item {
  display: block;
  width: 186rpx;
  height: 140rpx;
  overflow: hidden;
  position: relative;
  transition:all 0.5s;
  -moz-transition:all 0.5s;
  -webkit-transition:all 0.5s;
  -o-transition:all 0.5s;
}

.lotto-center .lotto-main .item .side-item.side-nagative {
  transform: rotateY(-180deg);
  opacity: 0;
  visibility: hidden;
  position: absolute;
  left: 0;
  top: 0;
}


.lotto-center .lotto-main .item .side-item .img {
  position: absolute;
  top: 0;
  left: 0;
  width: 186rpx;
  height: 140rpx;
  z-index: 0;
}

