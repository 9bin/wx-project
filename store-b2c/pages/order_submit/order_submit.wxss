/* pages/order_submit/order_submit.wxss */
@import "../../wxParse/wxParse.wxss";

view {
  overflow: visible;
}

.bottom-bar {
  position: fixed;
  bottom: 0;
  left: 0;
  height: 100rpx;
  background: #fff;
  border-top: 1rpx solid #e3e3e3;
  width: 100%;
  z-index: 1000;
}

.is_area {
  background: #febd14;
  color: #fff;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
}

.bottom-bar .submit-btn {
  background-color: #eb0c36;
  color: #fff;
  width: 250rpx;
  text-align: center;
  margin: 0;
  padding: 0;
  border-radius: 0;
}

.address-picker {
  background: #fff;
  padding: 32rpx 24rpx;
  margin-bottom: 20rpx;
  background-image: url("data:image/png;base64,");
  background-position: left bottom;
  background-repeat: repeat-x;
  background-size: 35rpx 5rpx;
}

.cart-item {
  background: #fff;
  padding: 32rpx 24rpx 10rpx 24rpx;
}

.cart-item .goods-pic {
  width: 156rpx;
  height: 156rpx;
  margin-right: 20rpx;
  border: 1rpx solid #eee;
}

.cart-item .goods-name {
  margin-bottom: 10rpx;
  -webkit-line-clamp: 2;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  overflow: hidden;
}

.cart-item .attr-list, .cart-item .num {
  font-size: 9pt;
  color: #888;
}

.cart-item .attr-list .attr-item {
  display: inline-block;
  margin-right: 25rpx;
}

.cart-item .attr-list .attr-item:last-child {
  margin-right: 0;
}

.cart-item .price {
  color: #ff0036;
  font-size: 32rpx;
}

.cart-item .orgin_price {
  color: #999;
  font-size: 26rpx;
  text-decoration: line-through;
  margin-left: 10rpx;
}

.goods-intrgral {
  height: 60rpx;
  background-color: #f8f8f8;
  width: 100%;
  border-bottom: 1rpx solid #e3e3e3;
  color: #919191;
  font-size: 9pt;
  padding: 0 24rpx;
}

.goods-gifts {
  height: 60rpx;
  background-color: #f8f8f8;
  width: 100%;
  border-bottom: 1rpx solid #e3e3e3;
  color: #919191;
  font-size: 9pt;
  padding: 0 24rpx;
}

.lable {
  width: 100%;
  padding: 24rpx;
  background: #fff;
  font-size: 28rpx;
}

.integral-switch .wx-switch-input {
  width: 42px;
  height: 22px;
  margin-left: 5rpx;
}

.integral-switch .wx-switch-input::before {
  width: 41px;
  height: 20px;
}

.integral-switch .wx-switch-input::after {
  width: 20px;
  height: 20px;
}

.alertMainer {
  margin-top: 20%;
}

.descContent {
  background: #fff;
  border-radius: 30rpx;
}

.descContent .title {
  height: 90rpx;
  font-size: 36rpx;
  font-weight: 550;
}

.descContent .content {
  padding: 24rpx;
}

.descContent .btn {
  height: 100rpx;
  font-size: 36rpx;
  font-weight: 550;
  border: none;
  border-radius: 0;
  border-top: 1rpx solid #e6e6e6;
}

.descContent .code {
  border: 1rpx solid #ccc;
  border-radius: 5rpx;
  height: 80rpx;
  font-size: 26rpx;
}

.descContent .code input {
  width: 240rpx;
  height: 80rpx;
  line-height: 80rpx;
  padding: 0 20rpx;
  border-right: 1rpx solid #ccc;
}

.descContent .code button {
  width: 240rpx;
  height: 80rpx;
  line-height: 80rpx;
  border: none;
  padding: 0;
  color: #eb0c36;
}

.descContent .balance {
  border-bottom: 1rpx solid #e6e6e6;
  border-top: 1rpx solid #e6e6e6;
  padding: 24rpx 0;
  width: 480rpx;
}

.descContent .balance .fi-rmb {
  font-size: 50rpx;
  color: #eca61f;
  margin-right: 5rpx;
}

.descContent .payInput {
  padding: 24rpx 0;
  position: relative;
  font-size: 72rpx;
  font-weight: 550;
  width: 480rpx;
}

.descContent .payInput .iptBalencePay {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  padding-left: 72rpx;
}

.descContent .payInput .text {
  position: absolute;
  height: 80rpx;
  line-height: 80rpx;
  left: 0;
}

.descContent .inputContent {
  position: relative;
  margin-top: 40rpx;
}

.descContent .inputContent .iptGroups {
  border: 1rpx solid #ccc;
  border-radius: 10rpx;
}

.descContent .inputContent .iptGroups .iptbox {
  width: 80rpx;
  height: 80rpx;
  display: flex;
  border-left: 1rpx solid #ccc;
  justify-content: center;
  align-items: center;
  text-align: center;
}

.descContent .inputContent .iptGroups .iptbox:first-child {
  border-left: none;
}

.descContent .inputContent .ipt {
  position: absolute;
  left: 0;
  width: 0;
  height: 0;
}

.descContent .forgetPwd {
  width: 480rpx;
  text-align: right;
  color: #999;
  font-size: 26rpx;
  margin-top: 24rpx;
}
